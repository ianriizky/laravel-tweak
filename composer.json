{
    "name": "ianriizky/laravel-tweak",
    "description": "Make your Laravel Framework become more powerful and IDE-readable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ianriizky",
            "email": "ian.rizkypratama@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "laravel",
        "tweak",
        "addon",
        "extension"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.5",
        "laravel/framework": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "files": [
            "src/Illuminate/Support/helpers.php"
        ],
        "psr-4": {
            "Ianrizky\\Illuminate\\": "src/Illuminate/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ianrizky\\LaravelTweak\\Tests\\": "tests/"
        }
    }
}
